{
    "background_page": "views/background.html",
    "browser_action": {
        "default_icon": "images/backgroundIcons/disabled.png",
        "default_title": "Grooveshark Control",
        "default_popup": "views/popup.html"
    },
    "content_scripts": [{
        "js": ["javascript/jquery-1.6.2.min.js", "javascript/contentscript.js"],
        "matches": ["http://grooveshark.com/*"]
    }],
    "description": "Grooveshark control.",
    "icons": {
        "16": "images/icon_16.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "name": "Grooveshark Control",
    "options_page": "views/options.html",
    "permissions": [
        "tabs", "notifications", "http://grooveshark.com/*"
    ],
    "version": "0.8.3"
}
